    "child_process": "^1.0.2",
    "highlight.js": "^11.9.0",
    "markdown-to-jsx": "^7.4.7",
    "ollama": "^0.5.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-highlight": "^7.0.0",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
